.window {
	border: 1px solid #346789;
	box-shadow: 2px 2px 19px #aaa;
	-o-box-shadow: 2px 2px 19px #aaa;
	-webkit-box-shadow: 2px 2px 19px #aaa;
	-moz-box-shadow: 2px 2px 19px #aaa;
	-moz-border-radius: 0.5em;
	border-radius: 0.5em;
	opacity: 0.8;
	filter: alpha(opacity =                                                         
		 80);
	width: 10.5em;
	height: 6em;
	line-height: 5em;
	text-align: center;
	z-index: 20;
	position: absolute;
	background-color: #eeeeef;
	color: black;
	font-family: helvetica;
	padding: 0.5em;
	font-size: 0.8em;
}

.state {
	border: 1px solid #346789;
	box-shadow: 2px 2px 19px #aaa;
	-o-box-shadow: 2px 2px 19px #aaa;
	-webkit-box-shadow: 2px 2px 19px #aaa;
	-moz-box-shadow: 2px 2px 19px #aaa;
	-moz-border-radius: 0.5em;
	border-radius: 0.5em;
	opacity: 0.8;
	filter: alpha(opacity =                                                         
		 80);
	width: 11em;
	height: 6em;
	line-height: 5em;
	text-align: center;
	z-index: 20;
	position: absolute;
	background-color: #eeeeef;
	color: black;
	font-family: helvetica;
	padding: 0.5em;
	font-size: 0.8em;
}

.aLabel {
	background-color: white;
	font: 18px sans-serif;
	color: #990000;
	z-index: 21;
	border: 1px dotted gray;
	opacity: 0.8;
	filter: alpha(opacity = 80);
}

._jsPlumb_endpoint {
	z-index: 3;
}

.dragHover {
	border: 1px dotted red;
}

.ep {
	position: absolute;
	top: 50%;
	right: 45%;
	width: 1em;
	height: 1em;
	background-color: #222288;
	cursor: pointer;
}

#lifeCycleChart {
	width: 120%;
	height: 240%;
	margin-left: 11em;
	margin-top: -1em;
	position: relative;
	background: #F0F8FF;
	font-size: 16px;
}

#lifeCycleChart.changeColor {
	background: #9999FF;
}

#lifeCycleChart  #sw {
	text-decoration: underline;
	cursor: pointer;
}

#Palette {
	width: 30%;
	height: 0em;
	font-size: 16px;
}

#1 {
	top: -4em;
	left: -4em;
}

#2 {
	top: -2em;
	left: -1em;
}

#3 {
	top: -2em;
	left: -1em;
}

.hl {
	border: 3px solid red;
}

.texte {
	border: 1px dotted gray;
}